{
    "name": "kerneldos/yii2-admin-modules",
    "type": "yii2-extension",
    "description": "The Yii2 extension of module management",
    "keywords": [
        "yii2",
        "extension",
        "kerneldos",
        "yii2-admin-modules"
    ],
    "homepage": "https://github.com/kerneldos/yii2-admin-modules",
    "license": "MIT",
    "authors": [
        {
            "name": "Mishin A. E.",
            "email": "kerneldos@mail.ru",
            "homepage": "https://github.com/kerneldos",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-gii": "~2.1.0",
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "kerneldos\\extmodule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kerneldos\\extmodule\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "bootstrap": "kerneldos\\extmodule\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
